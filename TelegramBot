# -*- coding: utf-8 -*-
import telebot


bot = telebot.TeleBot('1741238406:AAFl-wQQ-3RiGsIDcSdQxNLsURAu4gkYd-A')   #token


#–∫–ª–∞–≤—ñ–∞—Ç—É—Ä–∞
@bot.message_handler(commands=['start'])
def handle_start(message):
    user_markup = telebot.types.ReplyKeyboardMarkup(True, False)
    user_markup.row('–©–æ —Ç–∞–∫–µ –£–ú–•?ü§î','–Ü—Å—Ç–æ—Ä—ñ—è –£–ú–•üéì')
    user_markup.row('–ê–Ω–¥—Ä–µ–π –®–µ–ø—Ç–∏—Ü—å–∫–∏–πü§µ', '–ù–∞—à–∞ —Å–∏–º–≤–æ–ª—ñ–∫–∞üìï')
    user_markup.row('–ö–æ–Ω—Ç–∞–∫—Ç–∏üì≤')
    text='–ü—Ä–∏–≤—ñ—Ç '
    text1="""ü•≥.

–¢–µ–±–µ –∑–∞—Ü—ñ–∫–∞–≤–∏–ª–∞ –∞–±—Ä–µ–≤—ñ–∞—Ç—É—Ä–∞ - –£–ú–•? –Ø–∫—â–æ —Ç–∞–∫ —Ç–æ –∑–∞—Ä–∞–∑ —è –ø–æ—è—Å–Ω—é —Ç–æ–±—ñ, —â–æ —Ü–µ —ñ –∑ —á–∏–º –π–æ–≥–æ —ó–¥—è—Ç—å.üòè

–í–∏–±–µ—Ä—ñ—Ç—å –∑ –∑–∞–ø—Ä–æ–ø–æ–Ω–æ–≤–∞–Ω–∏—Ö –≤–∞—Ä—ñ–∞–Ω—Ç—ñ–≤üòÅ"""
    bot.send_message(str(message.from_user.id or '0'), text + str(message.from_user.first_name) + text1, reply_markup=user_markup)



@bot.message_handler(content_types=['text'])
def handle_text(message):
    print(message.text)

#–©–æ —Ç–∞–∫–µ –£–ú–•?'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
#---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
#---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
#---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
    if message.text == '–©–æ —Ç–∞–∫–µ –£–ú–•?ü§î':
        user_markup= telebot.types.ReplyKeyboardMarkup(True, False)
        user_markup.row('–ú–µ—Ç–∞ –æ—Ä–≥–∞–Ω—ñ–∑–∞—Ü—ñ—óüìå')
        user_markup.row('–ó–∞–≤–¥–∞–Ω–Ω—è –£–ú–•üìå')
        user_markup.row('–ù–∞ –≥–æ–ª–æ–≤–Ω—Éüîô')
        bot.send_message(message.from_user.id,"¬´–£–∫—Ä–∞—ó–Ω—Å—å–∫–∞ –º–æ–ª–æ–¥—å ‚Äî –•—Ä–∏—Å—Ç–æ–≤—ñ¬ª ‚Äî –≥—Ä–æ–º–∞–¥—Å—å–∫–∞ –æ—Ä–≥–∞–Ω—ñ–∑–∞—Ü—ñ—è, –∑–∞—Å–Ω–æ–≤–∞–Ω–∞ –º–∏—Ç—Ä–æ–ø–æ–ª–∏—Ç–æ–º –ê–Ω–¥—Ä–µ—î–º –®–µ–ø—Ç–∏—Ü—å–∫–∏–º —É –ì–∞–ª–∏—á–∏–Ω—ñ –≤ 1930-—Ö —Ä–æ–∫–∞—Ö." , reply_markup=user_markup)
    elif message.text == '–ú–µ—Ç–∞ –æ—Ä–≥–∞–Ω—ñ–∑–∞—Ü—ñ—óüìå':
        user_markup = telebot.types.ReplyKeyboardMarkup(True, False)
        bot.send_message(message.from_user.id, """–ú–µ—Ç–æ—é –¥—ñ—è–ª—å–Ω–æ—Å—Ç—ñ –£–ú–• —î –æ–±‚Äô—î–¥–Ω–∞–Ω–Ω—è —Ö—Ä–∏—Å—Ç–∏—è–Ω—Å—å–∫–æ—ó –º–æ–ª–æ–¥—ñ –¥–ª—è –ø—Ä–æ–ø–∞–≥—É–≤–∞–Ω–Ω—è —Ö—Ä–∏—Å—Ç–∏—è–Ω—Å—å–∫–æ—ó –º–æ—Ä–∞–ª—ñ, –ø–æ—à–∏—Ä–µ–Ω–Ω—è —Ö—Ä–∏—Å—Ç–∏—è–Ω—Å—å–∫–æ–≥–æ —Å–≤—ñ—Ç–æ–≥–ª—è–¥—É, –ø–æ–ø—É–ª—è—Ä–∏–∑–∞—Ü—ñ—è –≤ –º–æ–ª–æ–¥—ñ–∂–Ω–æ–º—É —Å–µ—Ä–µ–¥–æ–≤–∏—â—ñ —Ö—Ä–∏—Å—Ç–∏—è–Ω—Å—å–∫–æ–≥–æ —Å–ø–æ—Å–æ–±—É –∂–∏—Ç—Ç—è, —à–ª—è—Ö–æ–º –≤–∏—Ö–æ–≤–∞–Ω–Ω—è —á–ª–µ–Ω—Å—Ç–≤–∞ –Ω–∞ —Ö—Ä–∏—Å—Ç–∏—è–Ω—Å—å–∫–∏—Ö –∑–∞—Å–∞–¥–∞—Ö, –≥–æ—Ç–æ–≤–æ–≥–æ –ø—Ä–∞—Ü—é–≤–∞—Ç–∏ –¥–ª—è –¥–æ–±—Ä–∞ —Ü–µ—Ä–∫–≤–∏ —É–∫—Ä–∞—ó–Ω—Å—å–∫–æ–≥–æ –Ω–∞—Ä–æ–¥—É. 

            """)
    elif message.text == '–ó–∞–≤–¥–∞–Ω–Ω—è –£–ú–•üìå':
        user_markup = telebot.types.ReplyKeyboardMarkup(True, False)
        bot.send_message(message.from_user.id, """–ó–∞–≤–¥–∞–Ω–Ω—è–º –£–ú–• —î:
        
—Å–ø—Ä–∏—è–Ω–Ω—è —Ç–∞ –¥–æ–ø–æ–º–æ–≥–∞ –º–æ–ª–æ–¥—ñ —É –ø—Ä–∏–π–Ω—è—Ç—Ç—ñ —Ö—Ä–∏—Å—Ç–∏—è–Ω—Å—å–∫–∏—Ö —Ü—ñ–Ω–Ω–æ—Å—Ç–µ–π —Ç–∞ –ø—ñ–¥–≤–∏—â–µ–Ω–Ω—ñ —ó—ó –¥—É—Ö–æ–≤–Ω–æ–≥–æ —Ä—ñ–≤–Ω—è;
        
–≤—ñ–¥—Ä–æ–¥–∂–µ–Ω–Ω—è –¥–∞–≤–Ω—ñ—Ö —Ä–æ–¥–∏–Ω–Ω–æ-–ø–æ–±—É—Ç–æ–≤–∏—Ö —Ç—Ä–∞–¥–∏—Ü—ñ–π, –Ω–∞—Ä–æ–¥–Ω–∏—Ö —Ö—Ä–∏—Å—Ç–∏—è–Ω—Å—å–∫–∏—Ö –¥—ñ–π—Å—Ç–≤; 
        
–ø—Ä–æ–≤–∞–¥–∂–µ–Ω–Ω—è –≤ –º–æ–ª–æ–¥—ñ–∂–Ω–µ —Å–µ—Ä–µ–¥–æ–≤–∏—â–µ –¥–æ—Å—è–≥–Ω–µ–Ω—å —Å–≤—ñ—Ç–æ–≤–æ—ó —Ö—Ä–∏—Å—Ç–∏—è–Ω—Å—å–∫–æ—ó –Ω–∞—É–∫–∏ —Ç–∞ –∫—É–ª—å—Ç—É—Ä–∏;

–≤–∏—Ö–æ–≤–∞–Ω–Ω—è —É –º–æ–ª–æ–¥—ñ –ø–æ–≤–∞–≥–∏ –¥–æ –º–∏–Ω—É–ª–æ–≥–æ, –ø–æ—à—É–∫ —Ç–∞ –∑–±–µ—Ä–µ–∂–µ–Ω–Ω—è —ñ—Å—Ç–æ—Ä–∏—á–Ω–∏—Ö –¥–æ–∫—É–º–µ–Ω—Ç—ñ–≤, –¥—É—Ö–æ–≤–Ω–∏—Ö, –º–∏—Å—Ç–µ—Ü—å–∫–∏—Ö —Ç–∞ —ñ–Ω—à–∏—Ö —Ü—ñ–Ω–Ω–æ—Å—Ç–µ–π —É–∫—Ä–∞—ó–Ω—Å—å–∫–æ–≥–æ –Ω–∞—Ä–æ–¥—É. 

            """)
        #bot.send_message(message.from_user.id, """¬´–£–∫—Ä–∞—ó–Ω—Å—å–∫–∞ –º–æ–ª–æ–¥—å ‚Äî –•—Ä–∏—Å—Ç–æ–≤—ñ¬ª (–£–ú–•) ‚Äî –æ—Ä–≥–∞–Ω—ñ–∑–∞—Ü—ñ—è, –∑–∞—Å–Ω–æ–≤–∞–Ω–∞ –º–∏—Ç—Ä–æ–ø–æ–ª–∏—Ç–æ–º –ê–Ω–¥—Ä–µ—î–º –®–µ–ø—Ç–∏—Ü—å–∫–∏–º —É –ì–∞–ª–∏—á–∏–Ω—ñ –≤ 1930-—Ö —Ä–æ–∫–∞—Ö.""")
#–ù–∞ –≥–æ–ª–æ–≤–Ω—Éüîô------------------------------------------    
    elif message.text == '–ù–∞ –≥–æ–ª–æ–≤–Ω—Éüîô':
        user_markup = telebot.types.ReplyKeyboardMarkup(True, False)
        user_markup.row('–©–æ —Ç–∞–∫–µ –£–ú–•?ü§î','–Ü—Å—Ç–æ—Ä—ñ—è –£–ú–•üéì')
        user_markup.row('–ê–Ω–¥—Ä–µ–π –®–µ–ø—Ç–∏—Ü—å–∫–∏–πü§µ', '–ù–∞—à–∞ —Å–∏–º–≤–æ–ª—ñ–∫–∞üìï')
        user_markup.row('–ö–æ–Ω—Ç–∞–∫—Ç–∏üì≤')
        bot.send_message(message.from_user.id, "–í–∏–±–µ—Ä—ñ—Ç—å –∑ –∑–∞–ø—Ä–æ–ø–æ–Ω–æ–≤–∞–Ω–∏—Ö –≤–∞—Ä—ñ–∞–Ω—Ç—ñ–≤‚¨áÔ∏è", reply_markup=user_markup)
#–Ü—Å—Ç–æ—Ä—ñ—è –£–ú–•'---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
#---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
#---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
#---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
    if message.text == '–Ü—Å—Ç–æ—Ä—ñ—è –£–ú–•üéì':
        user_markup = telebot.types.ReplyKeyboardMarkup(True, False)
        user_markup.row('–Ü—Å—Ç–æ—Ä—ñ—è –í—Å–µ—É–∫—Ä–∞—ó–Ω—Å—å–∫–æ–≥–æ –£–ú–•üìå')
        user_markup.row('–ù–∞ –≥–æ–ª–æ–≤–Ω—Éüîô')
        bot.send_message(message.from_user.id, "–í–∏–±–µ—Ä—ñ—Ç—å –∑ –∑–∞–ø—Ä–æ–ø–æ–Ω–æ–≤–∞–Ω–∏—Ö –≤–∞—Ä—ñ–∞–Ω—Ç—ñ–≤‚¨áÔ∏è" , reply_markup=user_markup)
    elif message.text == '–Ü—Å—Ç–æ—Ä—ñ—è –í—Å–µ—É–∫—Ä–∞—ó–Ω—Å—å–∫–æ–≥–æ –£–ú–•üìå':
