    elif message.text == '–ï–º–±–ª–µ–º–∞ –£–ú–•üìå':
        user_markup = telebot.types.ReplyKeyboardMarkup(True, False)
        sti = open('umh.jpg', 'rb')
        bot.send_photo(message.chat.id, sti, """ –ï–º–±–ª–µ–º–∞ –í—Å–µ—É–∫—Ä–∞—ó–Ω—Å—å–∫–æ–≥–æ –£–ú–• """)
